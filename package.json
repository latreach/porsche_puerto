    "copy-webpack-plugin": "^3.0.1",
    "core-js": "^2.4.1",
    "css-loader": "^0.23.1",
    "dsv-loader": "^1.1.0",
    "eslint": "^3.1.1",
    "eslint-loader": "^1.4.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "font-awesome-loader": "^1.0.0",
    "html-webpack-plugin": "^2.22.0",
    "imports-loader": "^0.6.5",
    "json-loader": "^0.5.4",
    "lite-server": "^2.2.2",
